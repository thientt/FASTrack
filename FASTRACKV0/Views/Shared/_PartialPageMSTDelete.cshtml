@model FASTrack.Model.DTO.MSTDto

<div class="row">
    @using (Html.BeginForm())
    {
        <div class="form-horizontal">
            <div class="form-group">
                <label class="control-label col-lg-2">Name</label>
                <div class="col-lg-10">
                    @Html.TextBoxFor(model => model.Name, htmlAttributes: new { @class = "form-control", @readonly = "true" })
                </div>
            </div>
            <div class="form-group">
                <label class="col-lg-2 control-label">Description</label>
                <div class="col-lg-10">
                    @Html.TextBoxFor(model => model.Description, htmlAttributes: new { @class = "form-control", @readonly = "true" })
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-lg-2">Last Update By</label>
                <div class="col-lg-10">
                    @Html.TextBoxFor(model => model.LastUpdatedBy, htmlAttributes: new { @class = "form-control", @readonly = "true" })
                </div>
            </div>
            <div class="form-group">
                <label class="col-lg-2 control-label">Last Updated</label>
                <div class="col-lg-10">
                    @Html.DisplayFor(model => model.LastUpdate)
                </div>
            </div>
            <div class="form-group">
                <div class="col-lg-offset-2 col-lg-10">
                    <button class="btn btn-danger btn-width-md" name="btnSubmit" id="btnSubmit">Delete</button>
                </div>
            </div>
            <br />
            <div class="form-group">
                <div class="col-lg-offset-2 col-lg-10">
                    @Html.ActionLink("Back to List", "Index")
                </div>
            </div>
        </div>
    }
</div>
@Html.Partial("_PartialPageLoading")
@section Scripts {
}