@model FASTrack.Model.DTO.MSTProcessTypesDto

@{
    ViewBag.Title = "Details";
}

<h2>Process Type Details</h2>
<hr />

<div class="row">
    <div class="form-horizontal">
        <div class="form-group">
            <label class="control-label col-lg-2">Name </label>
            <div class="col-lg-10">
                @Html.TextBoxFor(model => model.Name, htmlAttributes: new { @class = "form-control", @readonly = "true" })
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-lg-2">Description</label>
            <div class="col-lg-10">
                @Html.TextBoxFor(model => model.Description, htmlAttributes: new { @class = "form-control", @readonly = "true" })
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-lg-2">Duration</label>
            <div class="col-lg-10">
                @Html.TextBoxFor(model => model.Duration, htmlAttributes: new { @class = "form-control", @readonly = "true" })
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-lg-2">SeqNumber</label>
            <div class="col-lg-10">
                @Html.TextBoxFor(model => model.SeqNumber, htmlAttributes: new { @class = "form-control", @readonly = "true" })
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-lg-2">Last Updated by</label>
            <div class="col-lg-10">
                @Html.TextBoxFor(model => model.LastUpdatedBy, htmlAttributes: new { @class = "form-control", @readonly = "true" })
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-lg-2">Last Update</label>
            <div class="col-lg-10">
                @Html.DisplayFor(model => model.LastUpdate)
            </div>
        </div>
        <br />
        <div class="form-group">
            <div class="col-lg-offset-2 col-lg-10">
                @Html.ActionLink("Edit", "Edit", new { id = Model.Id }) |
                @Html.ActionLink("Back to List", "Index")
            </div>
        </div>
    </div>
</div>
